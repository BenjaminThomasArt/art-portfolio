{
  "version": 2,
  "installCommand": "corepack enable && pnpm install --no-frozen-lockfile",
  "builds": [
    {
      "src": "api/index.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public",
        "installCommand": "corepack enable && pnpm install --no-frozen-lockfile"
      }
    }
  ],
  "routes": [
    { "src": "/api/trpc/(.*)", "dest": "/api/index.mjs" },
    { "src": "/api/oauth/(.*)", "dest": "/api/index.mjs" },
    { "src": "/api/health", "dest": "/api/index.mjs" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
