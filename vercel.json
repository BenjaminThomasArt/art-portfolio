{
  "version": 2,
  "buildCommand": "pnpm run vercel-build",
  "outputDirectory": "dist/public",
  "installCommand": "corepack enable && pnpm install --no-frozen-lockfile",
  "functions": {
    "api/index.mjs": {
      "memory": 256,
      "maxDuration": 10
    }
  },
  "rewrites": [
    { "source": "/api/trpc/:path*", "destination": "/api/index.mjs" },
    { "source": "/api/oauth/:path*", "destination": "/api/index.mjs" },
    { "source": "/api/health", "destination": "/api/index.mjs" },
    { "source": "/:path*", "destination": "/index.html" }
  ]
}
